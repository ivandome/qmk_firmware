OLED_ENABLE = no		# Enables the use of OLED displays
OLED_DRIVER = SSD1306   
ENCODER_ENABLE = no     # Enables the use of one or more encoders
RGBLIGHT_ENABLE = yes   # Enable keyboard RGB underglow
SLEEP_LED_ENABLE = no	# Enable your LED to breath while your computer is sleeping

# Console for debug, use `qmk console`
CONSOLE_ENABLE = no

# Enable to allows bidirectional comunication via Raw HID
RAW_ENABLE = no

# Enable to allows bidirectional comunication via Virtual Serial Port
# Use minicom --device /dev/ttyACM?
VIRTSER_ENABLE = no
